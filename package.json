{
    "name": "sudoku",
    "version": "1.0.0",
    "scripts": {
        "preinstall": "cd client && npm install && cd ../server && npm install",
        "client": "cd client && npm run dev",
        "server": "cd server && npm run server",
        "dev": "concurrently \"npm run server\" \"npm run client\""
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "concurrently": "^8.2.2"
    }
}
